# Environment variables for the RAG pipeline

# Qdrant configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=your_qdrant_api_key

# Embedding model configuration
EMBEDDING_MODEL_NAME=ai/embeddinggemma
EMBEDDING_DIM=768
EMBEDDING_MODEL_URL=http://localhost:12434/engines/llama.cpp/v1
GENERATION_MODEL_URL=http://localhost:12434/engines/llama.cpp/v1
GENERATION_MODEL_PROVIDER=openai
NEO4J_URI=http://localhost/llama.cpp/engines/v1
NEO4J_USERNAME=romeo
NEO4J_PASSWORD=none

# Generation model configuration
GENERATION_MODEL_NAME=ai/llama3.2:latest
